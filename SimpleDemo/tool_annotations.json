          { "TypesTaxonomy": ["PostScript"] }
        ],
        "outputs": { "TypesTaxonomy": ["PostScript"] },
        "implementation": { "code": "gmt psxy -R -J -O -Sc0.05 -Gred -Wthinnest $@input[0] >> $@input[1]\n@output[0]=$@input[1]" }
      },
